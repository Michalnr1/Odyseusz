@model IEnumerable<Odyseusz.domain.Kraj>

@{
    Layout = ViewData["Layout"] as string ?? "~/Views/Shared/_DatabaseLayout.cshtml";
}

<h2>Kraje</h2>

<table class="table">
    <thead>
        <tr>
            <th>Nazwa</th>
            <th>Kod</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var kraj in Model)
        {
            <tr>
                <td>@kraj.Nazwa</td>
                <td>@kraj.KodKraju</td>
            </tr>
        }
    </tbody>
</table>
